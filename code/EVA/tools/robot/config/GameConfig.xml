	<!--各种公式-->
	<formula>
	
		<score2money			     value="$a"/>											<!--积分墙积分转换到钱     $a=奖励积分     return=钱		          -->
		<lv_need_exp                 value="int(50*int($a/5)*int($a/5)*int($a/5))+(7*(($a*$a)/3)+(56*$a)+23-($a/2)*120)"/>                               <!--玩家等级对应的经验数   $a=等级         return=需要的经验数额	 (7*(($a*$a)/3)+(56*$a)+23-($a/2)*130)*100 -->
		<lv_up_money                 value="$a*100"/>                                  <!--升级给的钱数           $a=等级         return=奖励金币数额	      -->
		<lv_bouns_plus_money		 value="$a+($b*0)"/>                 <!-- $a=BONUS基本奖励   $b=玩家等级   return=BONUS金钱总数 -->
		<add_exp_slots               value="$a"/>                                <!-- $a=total_bet      return=增加经验数 -->
		<add_exp_texas               value="$a*10"/>                             <!-- $a=房间最小盲注   return=增加经验数 -->
		<add_exp_texas_chal          value="$a*10"/>                             <!-- $a=房间最小盲注   return=增加经验数 -->
		<add_exp_21                  value="$a*10"/>                             <!-- $a=房间最小筹码   return=增加经验数 -->
		<add_exp_21_chal             value="$a*10"/>                             <!-- $a=房间最小筹码   return=增加经验数 -->
		<mtt_money_circle			 value="$a*0.15+0.5"/>						 <!-- $a=总报名人数-->
		<add_exp_buygold             value="$a*0"/>					 <!-- $a=购买金币数  购买金币增加经验-->
		<add_exp_buyrmb              value="$a*0"/>					 <!-- $a=购买钻石数  购买人民币增加经验-->	
		<add_exp_mahjong_win		 value="3"/>								 <!-- 麻将胜利增加经验   -->
		<add_exp_mahjong_lost		 value="1"/>								 <!-- 麻将失败增加经验   -->				
		<add_exp_safe_box			 value="50"/>								 <!-- 设置保险柜增加经验 -->		
		<add_exp_buyvip		         value="$a*3"/>							     <!-- $a=购买VIP天数 购买经验增加经验   -->
		<horse_system_money          value="$a*0.1"/>                            <!-- 系统抽水 -->
		<horse_player_money          value="$a*0.9"/>                            <!-- 所有玩家分得钱 -->
		<horse_win_odd               value="$a*0.9/(3*$b)"/>                     <!-- win赔率算式,$a为player_money,$b为单格总下注 -->
		<horse_place_odd             value="$a*0.9/(6*$b)"/>                     <!-- place赔率算式中$a为player_money,$b为单格总下注 -->
		<horse_show_odd              value="$a*0.9/(9*$b)"/>                     <!-- show赔率算式中$a为player_money,$b为单格总下注 -->
		<horse_get_money             value="$a*$b"/>                             <!-- 赛马赢钱：$a为赔率，$b为该玩家下注 -->
		<horse_win_score             value="$a/$b*300"/>                         <!-- 赛马获取积分：$a为该玩家该列总产出，$b为该玩家该列总投入 -->
		<horse_place_score           value="$a/$b*200"/>                         <!-- 赛马获取积分：$a为该玩家该列总产出，$b为该玩家该列总投入 -->
		<horse_show_score            value="$a/$b*100"/>                         <!-- 赛马获取积分：$a为该玩家该列总产出，$b为该玩家该列总投入 -->
	</formula>

